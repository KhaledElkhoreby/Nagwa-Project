{
  "name": "nagwa",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Khaled El-Khoreby",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "swc src -d dist",
    "build-dev": "yarn run build && cross-env NODE_ENV=development node --inspect dist/index.js",
    "dev": "nodemon --ext ts --exec \"yarn run build-dev\"",
    "start": "yarn run build && cross-env NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.219",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.1",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  }
}
